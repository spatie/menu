{
    "name": "spatie/menu",
    "description": "Html menu generator",
    "keywords": [
        "spatie",
        "navigation",
        "menu"
    ],
    "homepage": "https://github.com/spatie/menu",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "spatie/html-element": "^1.0.0",
        "spatie/url": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2.10"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Menu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Menu\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
